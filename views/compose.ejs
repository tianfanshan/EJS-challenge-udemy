<%-include("partials/header")-%>

    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="postTitle" class="form-fabel">Title</label>
            <input id="postTitle" type="text" name="postTitle" class="form-control">
            <label for="post" class="form-fabel">Post</label>
            <textarea id="post" type="text" name="postBody" class="form-control" rows="3"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>


<%-include("partials/footer")-%>