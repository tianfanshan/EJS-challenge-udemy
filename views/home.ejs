<%-include("partials/header")%>

    <h1>Home</h1>
    <p><%= startingContent %></p>
    <%postsContent.map(post=>{%>
        <h1><%= post.title%></h1>
        <%if(post.content.length>100){%>
            <p><%= post.content.substring(0,100) + "..."%></p><a href="/posts/<%= post.title %>"> Read More</a>
            <%}else{%>
            <p><%= post.content%><a href="/posts/<%= post.title %>"> Read More</a></p>
            <%}%>
        <%})%>

<%- include("partials/footer") %>